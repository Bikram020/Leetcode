class Solution {
    public int minimumOperations(int[] nums) {
        int size = nums.length;
        int count = 0;
        for (int i = 0; i < size; i++) {
            if (nums[i] % 3 == 1) {
                count++;
            } else if (nums[i] % 3 == 2) {
                count++;
            }
        }
        return count;
    }
}
